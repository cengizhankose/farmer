{
  "name": "stacks-yield-agg",
  "private": true,
  "packageManager": "pnpm@10.16.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm -r --filter \"./packages/*\" run build && pnpm -r --filter \"./apps/*\" run build",
    "dev": "pnpm dev:web",
    "dev:web": "pnpm --filter @apps/web dev",
    "dev:mobile": "pnpm --filter @apps/mobile start",
    "dev:contracts": "pnpm --filter @contracts/core console",
    "test": "pnpm -r test",
    "test:contracts": "pnpm --filter @contracts/core test",
    "test:adapters": "pnpm --filter @adapters/core test",
    "lint": "pnpm -r lint",
    "lint:fix": "pnpm -r --parallel run lint -- --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "typecheck": "pnpm -r typecheck",
    "clean": "pnpm -r run clean && rm -rf node_modules",
    "reset": "pnpm clean && pnpm install",
    "contracts:check": "pnpm --filter @contracts/core check",
    "contracts:console": "pnpm --filter @contracts/core console",
    "setup": "pnpm install && pnpm build"
  },
  "keywords": ["stacks", "defi", "yield", "aggregator", "hackathon"],
  "author": "Stacks Yield Aggregator Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "typescript": "^5.3.0"
  }
}